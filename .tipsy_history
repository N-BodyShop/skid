openb dark.bin
openb dark.std
loadb 0
readarray dark.grp
pointsize dark 1
xarray all default 0 125
xarray all default 1 125 cliplo
quit
openb dark.std
loadb 0
readarray dark.grp
xarray all default 1 125 cliplo
readvector dark.ray
showvec yes
vscale 1
xarray all default 1 125 cliplo
pointsize dark 1
makebox 1
abox 1
xarray all default 1 125 cliplo
showvec no
xarray all default 1 125 cliplo
pointsize dark 2
plot\
plot
quit
